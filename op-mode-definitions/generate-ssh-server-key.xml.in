<?xml version="1.0"?>
<interfaceDefinition>
  <node name="generate">
    <properties>
      <help>Generate an object/key</help>
    </properties>
    <children>
      <node name="ssh">
        <properties>
          <help>Generate SSH related keypairs</help>
        </properties>
        <children>
          <node name="server-key">
            <properties>
              <help>Re-generate SSH host keys and restart SSH server</help>
            </properties>
            <command>sudo ${vyos_op_scripts_dir}/generate_ssh_server_key.py</command>
          </node>
          <tagNode name="client-key">
            <properties>
              <help>Re-generate SSH client keypair</help>
              <completionHelp>
                <list>&lt;filename&gt;</list>
              </completionHelp>
            </properties>
            <command>ssh-keygen -t rsa -f "$4" -N ""</command>
          </tagNode>
        </children>
      </node>
    </children>
  </node>
</interfaceDefinition>
